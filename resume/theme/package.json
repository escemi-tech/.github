{
  "name": "jsonresume-theme-escemi",
  "version": "2.0.0",
  "description": "A professional JSON Resume theme optimized for senior technical profiles following CV Coach best practices - React edition",
  "type": "module",
  "main": "./dist/jsonresume-theme-escemi.cjs",
  "module": "./dist/jsonresume-theme-escemi.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/jsonresume-theme-escemi.js",
      "require": "./dist/jsonresume-theme-escemi.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "tailwind.config.cjs",
    "postcss.config.cjs",
    "tsconfig.build.json"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage.enabled --coverage.reporter=lcov --coverage.reporter=text",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "author": "ESCEMI",
  "repository": {
    "type": "git",
    "url": "https://github.com/escemi-tech/.github"
  },
  "keywords": [
    "jsonresume",
    "theme",
    "cv",
    "resume",
    "technical",
    "senior",
    "cto",
    "lead-developer",
    "react",
    "cv-coach"
  ],
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "dependencies": {
    "@fontsource/source-sans-pro": "^5.1.0",
    "pagedjs": "^0.4.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.13",
    "autoprefixer": "^10.4.20",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.1",
    "react-to-print": "^3.0.5",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.13"
  }
}
