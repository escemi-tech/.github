{
  "name": "generate-resume-pdf",
  "version": "1.0.0",
  "private": true,
  "description": "Generates PDF resumes from JSON resume files.",
  "dependencies": {
    "jsonresume-theme-escemi": "file:../../../resume/themes/escemi",
    "resume-cli": "^3.1.2"
  },
  "scripts": {
    "generate-pdf": "RESUME_PUPPETEER_NO_SANDBOX=true resume export --theme escemi --format pdf --resume"
  }
}
